<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn" name="servebus" packages="">
    <Appenders>
        <File name="Log" fileName="logs/app.log">
            <PatternLayout>
                <Pattern>%d{yyyy-MM-dd HH:mm:ss} %-5p %c{1}:%L - %m%n</Pattern>
            </PatternLayout>
        </File>
    </Appenders>
    <Loggers>
        <Root level="error">
            <AppenderRef ref="Log"/>
        </Root>
    </Loggers>
</Configuration>